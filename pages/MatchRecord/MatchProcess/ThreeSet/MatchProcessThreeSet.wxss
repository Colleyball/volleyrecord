page,.page {
  font-family: 'PingFang SC', 'Helvetica Neue' ,Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
}

.container{
  padding-top:5rpx;
  margin: 0;
  background-color: rgba(228, 226, 228, 1)
}

.cover{
  position:fixed;
  width:100%;
  height:100%;
  top:0px;
  background:rgba(255,255,255,0.8);
  overflow: hidden;
}

.topbar{
  width: 100%;
  display: flex;
  flex-direction: row;
  text-align: center;
  height: 100rpx;
  margin-top: 5rpx;
}

.TeamAName{
  height: 100rpx;
  line-height: 100rpx;
  width: 340rpx;
  border-radius: 0rpx;
  margin-left:0rpx;
  color: black;
  font-size: 30rpx
}

.changecourt{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: #fff;
  box-shadow: 0rpx 0rpx 15rpx rgba(0, 0, 0, 0.1)
}

.TeamBName{
  height: 100rpx;
  line-height: 100rpx;
  width: 345rpx;
  border-radius: 0rpx;
  color: black;
  font-size: 30rpx
}

.Score {
  width: 680rpx;
  height: 200rpx;
  display: flex;
  flex-direction: row;
  text-align: center
}

.TeamAScore {
  width: 240rpx;
  height: 200rpx;
  background: #fff;
  border-radius: 5rpx;
  line-height: 200rpx;
  font-size: 80rpx;
  box-shadow: 0rpx 0rpx 15rpx rgba(0, 0, 0, 0.1)
}

.Setinfo {
  width: 200rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column
}

.TeamBScore {
  width: 240rpx;
  height: 200rpx;
  background: #fff;
  border-radius: 5rpx;
  line-height: 200rpx;
  font-size: 80rpx;
  box-shadow: 0rpx 0rpx 15rpx rgba(0, 0, 0, 0.1)
}

.Set {
  width: 200rpx;
  height: 100rpx;
  display: flex;
  flex-direction: row
}

.TeamASet {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
}

.TeamBSet {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
}

.ball {
  margin-left: 10rpx;
  width: 180rpx;
  height: 100rpx;
  display: flex;
  flex-direction: row
}

.winball {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  background-color: #fff;
  box-shadow: 0rpx 0rpx 15rpx rgba(0, 0, 0, 0.1)
}

.ball-pic {
  width: 70rpx;
  height: 70rpx;
  margin: 10rpx 0 0 0rpx;
}

.tool {
  width: 680rpx;
  height: 150rpx;
  display: flex;
  flex-direction: row;
  align-items:  center;
}

.tool-item {
  margin-left:30rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: #fff;
  text-align: center;
  box-shadow: 0rpx 0rpx 15rpx rgba(0, 0, 0, 0.1)
}

.tool-pic {
  width: 60rpx;
  height: 60rpx;
  margin: 20rpx 0 0 0rpx;
}

.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 40px;
  background-color: #fff;
  box-shadow: 0rpx 10rpx 20rpx rgba(0, 0, 0, 0.1);
  margin-bottom: 15rpx;
}

.title {
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 30rpx;
}

.swiper-tab-list {
  font-size: 28rpx;
  display: inline-block;
  width: 25%;
  color: #777;
  margin-bottom: 10rpx;
}

.on {
  color: #000;
  font-size: 30rpx;
  border-bottom:5rpx solid #000;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
  padding-top: 0rpx;
}

.swiper-box view {
  text-align: center;
}

.volleyball-court {
  width: 100%;
  height: 522rpx;
}

.volleyball-court-position {
  margin-top: 0rpx;
  width: 750rpx;
  height: 522rpx;
  margin-left: 0rpx;
  background: rgba(255, 255, 255, 1);
  display: flex;
  flex-direction: column;
  background: url('https://volleywang.cn/liansaiquan/images/wechat/rr/indoor_court.png') no-repeat;
  background-size: contain;
  background-position: center;
}

.line {
  width: 650rpx;
  margin-left: 50rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-top:70rpx
}

.player {
  width: 130rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 0rpx;
  background: #fff;
  border-radius: 5rpx;
  box-shadow: 0rpx 10rpx 20rpx rgba(0, 0, 0, 0.1);
}

.bottombar {
  position: fixed;
  height: 40rpx;
  line-height: 40rpx;
  width: 100%;
  bottom: 0;
  text-align: center;
  font-size: 25rpx;
  background: #fff;
  box-shadow: 0rpx -10rpx 15rpx rgba(0, 0, 0, 0.1);
}

.Statistic {
  position:fixed;
  top: 50rpx;
  width: 100%;
  height: 1050rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  justify-content: space-between;
  align-items: center;
}

.Statistic-item {
  width: 680rpx;
  height: 200rpx;
  line-height:200rpx;
  color: white;
  font-size: 50rpx;
  border-radius: 10rpx;
  box-shadow: 0rpx 10rpx 20rpx rgba(0, 0, 0, 0.1);
  background:rgba(0, 145, 235, 1)
}

.Statistic-fault{
  width: 680rpx;
  height: 412.5rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.Statistic-fault-line {
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.Statistic-fault-item {
  width: 333.75rpx;
  height: 200rpx;
  line-height:200rpx;
  color: white;
  font-size: 50rpx;
  border-radius: 10rpx;
  background:rgba(255, 92, 86, 1);
  box-shadow: 0rpx 10rpx 20rpx rgba(0, 0, 0, 0.1);
}

.Statistic-fault-forget {
  background:rgba(26, 45, 39, 1);
}

.Statistic-Court {
  position:fixed;
  top: 50rpx;
  width: 680rpx;
  margin-left: 35rpx;
  height: 650rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  justify-content: space-between;
  align-items: center;
}

.Statistic-Court-title {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 50rpx;
  border-radius: 10rpx;
  background:rgba(241, 170, 166, 1);
  box-shadow: 0rpx 10rpx 20rpx rgba(0, 0, 0, 0.1);
  width: 100%;
}

.Statistic-Court-line {
  height: 250rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.Statistic-Court-item {
  width: 200rpx;
  height: 250rpx;
  line-height: 250rpx;
  font-size: 100rpx;
  border-radius: 10rpx;
  color: white;
  background:rgba(241, 170, 166, 1);
  box-shadow: 0rpx 10rpx 20rpx rgba(0, 0, 0, 0.1);
}

.Statistic-Court-item-none {
  background:rgba(96, 105, 104, 0.5);
  font-size: 50rpx;
}

.TimeOut {
  position: fixed;
  top: 300rpx;
  margin-left: 75rpx;
  width: 600rpx;
  height: 200rpx;
  line-height: 200rpx;
  border-radius: 10rpx;
  text-align: center;
  font-size: 80rpx;
  color: black;
  background:white;
  box-shadow: 0rpx 10rpx 20rpx rgba(0, 0, 0, 0.1);
  
}

.TimeOutInfo {
  width: 100%;
  display: flex;
  flex-direction: column
}

.TimeOutInfo-line {
  width: 680rpx;
  margin-left: 35rpx;
  margin-bottom: 0rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 30rpx;
  color: black;
  background:white;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1)
}

.confirm-btn {
  position: fixed;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  bottom: 20rpx;
  left: 300rpx;
  line-height: 150rpx;
  text-align: center;
  font-size: 80rpx;
  background: #1A2D27;
  color: rgba(255, 255, 255, 1);
}

.matchdetail {
  width: 750rpx;
  overflow:hidden;
  zoom:1;
  height: 160rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1)
}

.matchdetail-socre {
  width: 35rpx;
  height: 80rpx;
  font-size: 26rpx;
  float: left;
  text-align: center;
}

.matchdetail-A {
  height: 40rpx;
  line-height: 40rpx;
}

.matchdetail-B {
  height: 40rpx;
  line-height: 40rpx;
}

.editScore {
  position: fixed;
  width: 500rpx;
  left: 125rpx;
  height: 400rpx;
  top:200rpx;
}

.edit-box {
  font-size: 80rpx;
  display: flex;
  width:500rpx;
  height:300rpx;
  margin-left: 0rpx;
  line-height: 300rpx;
  flex-direction: row;
  background-color: whitesmoke;
  border-radius: 20rpx;
  margin-top: 10rpx;
  margin-bottom: 30rpx;
  text-align: center;
  box-shadow: 0rpx 0rpx 15rpx rgba(0, 0, 0, 0.1)
}

.edit-confirm {
  width: 500rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 20rpx;
  margin-top: 10rpx;
  margin-bottom: 30rpx;
  text-align: center;
  box-shadow: 0rpx 0rpx 15rpx rgba(0, 0, 0, 0.1);
  background-color:whitesmoke;
  border:2rpx solid rgba(0, 0, 0, 0.5)
}


.TeamAColor {
  background-color: #25c6fc
}