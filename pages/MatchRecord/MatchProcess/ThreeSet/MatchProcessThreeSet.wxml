<view class="bottombar">
  {{MatchInfo[0]}}
</view>

<view class="cover" hidden="{{Statisticflag}}"></view>
<view class="Statistic" hidden="{{Statisticflag}}">
  <view class="Statistic-item" bindtap='StatisticPoint' data-point='SpikePoint' data-score='0'>进攻得分</view>
  <view class="Statistic-item" bindtap='StatisticPoint' data-point='BlockPoint' data-score='0'>拦网得分</view>
  <view class="Statistic-item" bindtap='StatisticPoint' data-point='ServePoint' data-score='0'>发球得分</view>
  <view class="Statistic-fault">
    <view class="Statistic-fault-line">
      <view class="Statistic-fault-item" bindtap='StatisticPoint' data-point='SpikeFault' data-score='1'>进攻失误</view>
      <view class="Statistic-fault-item" bindtap='StatisticPoint' data-point='ServeFault' data-score='1'>发球失误</view>
    </view>
    <view class="Statistic-fault-line">
      <view class="Statistic-fault-item" bindtap='StatisticPoint' data-point='OtherFault' data-score='1'>其他失误</view>
      <view class="Statistic-fault-item Statistic-fault-forget" bindtap='StatisticPoint' data-score='2'>我忘记了</view>
    </view>
  </view>
</view>

<view class="cover" hidden="{{ShowTeamAPlayer}}"></view>
<view class="Statistic-Court" hidden="{{ShowTeamAPlayer}}">
  <view class="Statistic-Court-title">{{MatchInfo[1]}}-{{Point}}</view>
  <view class="Statistic-Court-line">
    <view class="Statistic-Court-item　{{Point != 'BlockPoint' && Point != 'SpikePoint'  && Point != 'SpikeFault' && Point != 'OtherFault' ? 'Statistic-Court-item-none' : ''}}" bindtap="SetStatistic" data-position='{{TeamACourtPlayer[4]}}'>{{TeamACourtPlayer[4]}}</view>
    <view class="Statistic-Court-item {{Point != 'BlockPoint' && Point != 'SpikePoint'  && Point != 'SpikeFault' && Point != 'OtherFault' ? 'Statistic-Court-item-none' : ''}}" bindtap="SetStatistic" data-position='{{TeamACourtPlayer[3]}}'>{{TeamACourtPlayer[3]}}</view>
    <view class="Statistic-Court-item {{Point != 'BlockPoint' && Point != 'SpikePoint'  && Point != 'SpikeFault' && Point != 'OtherFault' ? 'Statistic-Court-item-none' : ''}}" bindtap="SetStatistic" data-position='{{TeamACourtPlayer[2]}}'>{{TeamACourtPlayer[2]}}</view>
  </view>
  <view class="Statistic-Court-line">
    <view class="Statistic-Court-item {{Point == 'ServeFault' || Point == 'ServePoint' || Point == 'BlockPoint' && Point != 'SpikeFault'? 'Statistic-Court-item-none' : ''}}" bindtap="SetStatistic" data-position='{{TeamACourtPlayer[5]}}'>{{TeamACourtPlayer[5]}}</view>
    <view class="Statistic-Court-item {{Point == 'ServeFault' || Point == 'ServePoint' || Point == 'BlockPoint' && Point != 'SpikeFault'? 'Statistic-Court-item-none' : ''}}" bindtap="SetStatistic" data-position='{{TeamACourtPlayer[6]}}'>{{TeamACourtPlayer[6]}}</view>
    <view class="Statistic-Court-item {{Point == 'BlockPoint' && Point != 'SpikeFault'? 'Statistic-Court-item-none' : ''}}" bindtap="SetStatistic" data-position='{{TeamACourtPlayer[1]}}'>{{TeamACourtPlayer[1]}}</view>
  </view>
</view>

