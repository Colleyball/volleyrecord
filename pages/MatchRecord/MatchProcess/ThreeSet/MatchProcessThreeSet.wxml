  <action-sheet hidden="{{ShowTeamAALLPlayer}}" bindchange="actionSheetbindchange">
    <action-sheet-item>请填写首发队员</action-sheet-item>
    <block wx:for-items="{{TeamAPlayer}}" wx:key="{{index}}">
      <block wx:if="{{TeamACourtPlayer[1] != item && TeamACourtPlayer[2] != item && TeamACourtPlayer[3] != item && TeamACourtPlayer[4] != item && TeamACourtPlayer[5] != item &&TeamACourtPlayer[6] != item}}">
        <action-sheet-item bindtap="SetTeamAPlayer" data-num='{{item}}'>{{item}}</action-sheet-item>
      </block>
    </block>
    <action-sheet-cancel class="cancel">取消</action-sheet-cancel>
</action-sheet>

  <action-sheet hidden="{{ShowTeamBALLPlayer}}" bindchange="actionSheetbindchange">
