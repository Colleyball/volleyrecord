<view class="container" hidden='{{Side}}'>
  <view class="topbar">
    <view class="TeamAName">{{MatchInfo[1]}}</view>
    <view class="changecourt" bindtap='ChangeSide'><image src='../../../../images/ic_swap.png' class="tool-pic" mode='aspectFit'></image></view>
    <view class="TeamBName">{{MatchInfo[2]}}</view>
  </view>
  <view class="Score">
    <view class="TeamAScore TeamAColor" bindtap="AddTeamAScore" bindlongpress="EditAScore">{{AScore}}</view>
    <view class="Setinfo">
      <view class="Set">
        <view class="TeamASet">{{AMatch}}</view>
        <view class="TeamBSet">{{BMatch}}</view>
      </view>
      <view class="ball">
        <view wx:if="{{LastScore == 0}}" class="winball" bindtap='ChangeServe'>
          <image src='../../../../images/ic_volley_ball_dark.png' class="ball-pic" mode='aspectFit'></image>
        </view>
        <view wx:if="{{LastScore == 1}}" class="winball" bindtap='ChangeServe' style='margin-left:90rpx'>
          <image src='../../../../images/ic_volley_ball_dark.png' class="ball-pic" mode='aspectFit'></image>
        </view>
      </view>
    </view>
    <view class="TeamBScore" bindtap="AddTeamBScore" bindlongpress="EditBScore">{{BScore}}</view>
  </view>
  <view class="tool">
    <view class="tool-item TeamAColor" bindtap="TimeOut" data-team='0'>    
      <image src='../../../../images/ic_timeout_tab.png' class="tool-pic" mode='aspectFit'></image>
    </view>
    <view class="tool-item TeamAColor" bindtap='ChangeTeamAPosition'>    
      <image src='../../../../images/refresh-light.png' class="tool-pic" mode='aspectFit'></image>
    </view>
    <view class="tool-item">    
      <image src='../../../../images/menu.png' bindtap="BindHelp" class="tool-pic" mode='aspectFit'></image>
    </view>
    <view class="tool-item" bindtap='ChangeTeamBPosition'>    
      <image src='../../../../images/refresh.png' class="tool-pic" mode='aspectFit'></image>
    </view>
    <view class="tool-item" bindtap="TimeOut" data-team='1'>    
      <image src='../../../../images/ic_timeout_tab_dark.png' class="tool-pic" mode='aspectFit'></image>
    </view>
  </view>
</view>

<view class="title" hidden='{{Side}}'>比赛信息
</view>
  <view class="swiper-tab" hidden='{{Side}}'>
    <view class="swiper-tab-list {{currentTab==0 ? 'on' : ''}}" data-current="0" bindtap="swichNav">站位</view>
    <view class="swiper-tab-list {{currentTab==1 ? 'on' : ''}}" data-current="1" bindtap="swichNav">进程</view>
    <view class="swiper-tab-list {{currentTab==2 ? 'on' : ''}}" data-current="2" bindtap="swichNav">暂停</view>
    <view class="swiper-tab-list {{currentTab==3 ? 'on' : ''}}" data-current="3" bindtap="swichNav">换人</view>
  </view>
  <view class="swiper-tab-temp" hidden='{{Side}}'></view>
  <view hidden='{{Side}}'>
    <swiper current="{{currentTab}}" class="swiper-box" style="height:540rpx" bindchange="bindChange">
      <swiper-item>
        <view class="volleyball-court-position">
          <view class="line">
            <view class="player TeamAColor"  bindlongpress='ChooseTeamAPlayer' data-position='5'>{{TeamACourtPlayer[5]}}
            </view>
            <view class="player TeamAColor" bindlongpress='ChooseTeamAPlayer' data-position='4'>{{TeamACourtPlayer[4]}}
            </view>
            <view class="player" bindlongpress='ChooseTeamBPlayer' data-position='2'>{{TeamBCourtPlayer[2]}}
            </view>
            <view class="player" bindlongpress='ChooseTeamBPlayer' data-position='1'>{{TeamBCourtPlayer[1]}}
            </view>
          </view>
          <view class="line">
            <view class="player TeamAColor" bindlongpress='ChooseTeamAPlayer' data-position='6'>{{TeamACourtPlayer[6]}}
            </view>
            <view class="player TeamAColor" bindlongpress='ChooseTeamAPlayer' data-position='3'>{{TeamACourtPlayer[3]}}
            </view>
            <view class="player" bindlongpress='ChooseTeamBPlayer' data-position='3'>{{TeamBCourtPlayer[3]}}
            </view>
            <view class="player" bindlongpress='ChooseTeamBPlayer' data-position='6'>{{TeamBCourtPlayer[6]}}
            </view>
          </view>
          <view class="line">
            <view class="player TeamAColor" bindlongpress='ChooseTeamAPlayer' data-position='1'>{{TeamACourtPlayer[1]}}
            </view>
            <view class="player TeamAColor" bindlongpress='ChooseTeamAPlayer' data-position='2'>{{TeamACourtPlayer[2]}}
            </view>
            <view class="player" bindlongpress='ChooseTeamBPlayer' data-position='4'>{{TeamBCourtPlayer[4]}}
            </view>
            <view class="player" bindlongpress='ChooseTeamBPlayer' data-position='5'>{{TeamBCourtPlayer[5]}}
            </view>
          </view>
        </view>
      </swiper-item>
     <swiper-item>
        <view class="matchdetail">
          <block wx:for="{{MatchDuratoinSet1}}" wx:key="{{index}}">
            <view class="matchdetail-socre">
              <view class="matchdetail-A">{{item[0]}}</view>
              <view class="matchdetail-B">{{item[1]}}</view>
            </view>
          </block>
        </view>
        <view class="matchdetail">
          <block wx:for="{{MatchDuratoinSet2}}" wx:key="{{index}}">
            <view class="matchdetail-socre">
              <view class="matchdetail-A">{{item[0]}}</view>
              <view class="matchdetail-B">{{item[1]}}</view>
            </view>
          </block>
        </view>
        <view class="matchdetail">
          <block wx:for="{{MatchDuratoinSet3}}" wx:key="{{index}}">
            <view class="matchdetail-socre">
              <view class="matchdetail-A">{{item[0]}}</view>
              <view class="matchdetail-B">{{item[1]}}</view>
            </view>
          </block>
        </view>
      </swiper-item>
      <swiper-item>
        <view class="TimeOutInfo">
          <view class="TimeOutInfo-line">
            {{MatchInfo[1]}}：本局比赛剩余{{TeamACurrentSetTimeOut}}次暂停
          </view>
          <view class="TimeOutInfo-line">
            {{MatchInfo[2]}}：本局比赛剩余{{TeamBCurrentSetTimeOut}}次暂停
          </view>
          <block wx:for="{{TimeOut}}" wx:key="{{index}}">
            <view class="TimeOutInfo-line">
              <block wx:if="{{item[0] == 0}}">
                【{{MatchInfo[1]}}】{{item[1]}}-{{item[2]}} {{item[3]}}:{{item[4]}}
              </block>
              <block wx:if="{{item[0]== 1}}">
                【{{MatchInfo[2]}}】{{item[1]}}-{{item[2]}} {{item[3]}}:{{item[4]}}
              </block>
            </view>
          </block>
        </view>
      </swiper-item>
      <swiper-item>
        <view class="TimeOutInfo">
          <view class="TimeOutInfo-line">
            {{MatchInfo[1]}}：本局比赛剩余{{TeamACurrentSetSub}}次换人
          </view>
          <view class="TimeOutInfo-line">
            {{MatchInfo[2]}}：本局比赛剩余{{TeamBCurrentSetSub}}次换人
          </view>
          <block wx:for="{{SubTeamASet3}}" wx:key="{{index}}">
            <view class="TimeOutInfo-line">
                【{{MatchInfo[1]}}】{{item[1]}}-{{item[2]}} {{item[3]}}:{{item[4]}} {{item[5]}}号下 {{item[6]}}号上
            </view>
          </block>
          <block wx:for="{{SubTeamBSet3}}" wx:key="{{index}}">
            <view class="TimeOutInfo-line">
                【{{MatchInfo[2]}}】{{item[1]}}-{{item[2]}} {{item[3]}}:{{item[4]}} {{item[5]}}号下 {{item[6]}}号上
            </view>
          </block>
          <block wx:for="{{SubTeamASet2}}" wx:key="{{index}}">
            <view class="TimeOutInfo-line">
                【{{MatchInfo[1]}}】{{item[1]}}-{{item[2]}} {{item[3]}}:{{item[4]}} {{item[5]}}号下 {{item[6]}}号上
            </view>
          </block>
          <block wx:for="{{SubTeamBSet2}}" wx:key="{{index}}">
            <view class="TimeOutInfo-line">
                【{{MatchInfo[2]}}】{{item[1]}}-{{item[2]}} {{item[3]}}:{{item[4]}} {{item[5]}}号下 {{item[6]}}号上
            </view>
          </block>
          <block wx:for="{{SubTeamASet1}}" wx:key="{{index}}">
            <view class="TimeOutInfo-line">
                【{{MatchInfo[1]}}】{{item[1]}}-{{item[2]}} {{item[3]}}:{{item[4]}} {{item[5]}}号下 {{item[6]}}号上
            </view>
          </block>
          <block wx:for="{{SubTeamBSet1}}" wx:key="{{index}}">
            <view class="TimeOutInfo-line">
                【{{MatchInfo[2]}}】{{item[1]}}-{{item[2]}} {{item[3]}}:{{item[4]}} {{item[5]}}号下 {{item[6]}}号上
            </view>
          </block>
        </view>
      </swiper-item>
    </swiper>
  </view>


