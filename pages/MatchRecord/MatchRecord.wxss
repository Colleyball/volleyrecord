	padding-left: 35rpx;
}

.on {
	color: rgba(0, 0, 0, 0.95);
	font-size: 70rpx;
	font-weight: bold;
}

.swiper-box {
	display: block;
	height: 100%;
	width: 100%;
	overflow: hidden;
	padding-top: 0rpx;
}

