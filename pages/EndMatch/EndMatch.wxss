page,.page {
  font-family: 'PingFang SC', 'Helvetica Neue' ,Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
}

.container {
	padding-top: 5rpx;
	margin: 0;
	background-color: rgba(255, 255, 255, 1)
}

.Setinfo {
	width: 100%;
	height: 220rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: column;
	flex-direction: column;
	-webkit-justify-content: space-between;
	justify-content: space-between;
	margin: 20rpx 0;
	text-align: center
}

.Setinfo-teamA {
	width: 600rpx;
	height: 100rpx;
	line-height: 100rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: row;
	flex-direction: row;
}

.Setinfo-teamA-name {
	width: 500rpx;
	background: rgba(255, 92, 86, 1);
	color: white;
	font-size: 40rpx;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.Setinfo-teamA-set {
	width: 100rpx;
	background: #1A1A1A;
	color: white;
	font-size: 70rpx;
}

.Setinfo-teamB {
	width: 600rpx;
	margin-left: 150rpx;
	height: 100rpx;
	line-height: 100rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: row;
	flex-direction: row;
	color: white;
	font-size: 40rpx;
}

.Setinfo-teamB-name {
	width: 500rpx;
	background: rgba(0, 145, 235, 1);
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.Setinfo-teamB-set {
	width: 100rpx;
	background: #1A1A1A;
	color: white;
	font-size: 70rpx;
}

.tablename {
	border-bottom: 2px solid whitesmoke;
	width: 750rpx;
	height: 50rpx;
	line-height: 50rpx;
	text-align: center;
	font-size: 30rpx;
}

.Scoreinfo {
	height: auto;
	width: 100%;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: column;
	flex-direction: column;
	-webkit-justify-content: space-between;
	justify-content: space-between;
}

.Scoreinfo-score {
	font-size: 40rpx;
	width: 680rpx;
	height: 80rpx;
	line-height: 80rpx;
	background: linear-gradient(45deg,rgba(31, 174, 221,0.05),rgba(255,83,77,0.05));
	border: 1rpx solid rgba(0, 0, 0, 0.1);
	border-radius: 0rpx;
	margin: 20rpx 0 20rpx 35rpx;
	text-align: center;
}

.back {
  position: fixed;
  bottom: 35rpx;
  left: 35rpx;
  width: 680rpx;
  background: linear-gradient(45deg,rgba(31, 174, 221,0.05),rgba(255,83,77,0.05));
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  color: #1A1A1A
}

.back:hover {
  background: linear-gradient(45deg,rgba(25, 152, 194, 0.1),rgba(238, 50, 44, 0.1));
}


.btnImg {
  margin-right: 8rpx;
  width: 46rpx;
  height: 46rpx;
}

.back::after {
  border-radius: 98rpx;
}

.upload-tool {
  position: fixed;
  bottom: 20rpx;
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around
}

.upload-tool-item {
	height: 100rpx;
  width: 100rpx;
	border-radius: 50%;
	text-align: center;
	font-size: 80rpx;
	box-shadow: 0rpx 0rpx 10rpx rgba(0, 0, 0, 0.1);
	background: linear-gradient(45deg,rgba(31, 174, 221,0.05),rgba(255,83,77,0.05));
	color: rgba(0, 0, 0, 0.8);
}
