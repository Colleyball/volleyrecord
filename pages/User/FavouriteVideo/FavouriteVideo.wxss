page,.page {
	font-family: “PingFang SC“, “Helvetica Neue“ ,Helvetica, “Droid Sans Fallback“, “Microsoft Yahei“, sans-serif;
	background: linear-gradient(45deg,rgba(37,198,252,0),rgba(246, 214, 255, 0),rgba(156, 211, 250, 0));
}

.swiper-tab {
	width: 100%;
	height: 120rpx;
	text-align: left;
	line-height: 50rpx;
	position: fixed;
	top: 0;
	z-index: 20;
}

.swiper-scroll-top {
	background: linear-gradient(0,rgba(255,255,255,0.1),rgba(31, 174, 221,0.2));
	border-bottom: 0rpx solid rgba(0, 0, 0, 0.1)
}

.swiper-scroll-down {
	background: rgba(255,255,255,0.95);
	border-bottom: 1rpx solid rgba(0, 0, 0, 0.1)
}

.swiper-tab-list {
	line-height: 120rpx;
	font-size: 40rpx;
	display: inline-block;
	width: auto;
	color: rgba(0, 0, 0, 0.4);
	padding-left: 35rpx;
}

.on {
	color: rgba(0, 0, 0, 0.95);
	font-size: 70rpx;
	font-weight: bold;
}

.swiper-box {
	display: block;
	height: 100%;
	width: 100%;
	overflow: hidden;
	padding-top: 0rpx;
}

.list {
	width: 680rpx;
	margin: 150rpx 0 0 35rpx;
}

.no-data {
	width: 450rpx;
	height: auto;
	padding: 0rpx;
	text-align: center;
	color: orange;
	line-height: 80rpx;
	position: fixed;
	top: 300rpx;
	left: 150rpx;
	border-radius: 40rpx;
	border: 1rpx solid gray
}

.video-box {
	width: 100%;
	height: 200rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: column;
	flex-direction: column;
	background: linear-gradient(45deg,rgba(31, 174, 221,0.05),rgba(255,83,77,0.05));
	border-radius: 0rpx;
	border: 1rpx solid rgba(0, 0, 0, 0.1);
	color: rgba(0, 0, 0, 0.9);
	text-align: center;
	-webkit-align-items: center;
	align-items: center;
	margin: 0;
	position: relative;
	z-index: 9
}

.video-title {
	width: 95%;
	height: 120rpx;
	line-height: 120rpx;
	font-size: 35rpx;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.video-detail {
	width: 60%;
	word-break: break-all;
	text-overflow: ellipsis;
	overflow: hidden;
	height: 80rpx;
	line-height: 80rpx;
	font-size: 30rpx;
	border-top: 1rpx solid rgba(0, 0, 0, 0.1)
}

.video-box-bg {
	width: 680rpx;
	height: 200rpx;
	margin: -202rpx 0 35rpx 0;
	opacity: 0.3;
	position: relative;
	z-index: 8
}

.match-result {
	width: 680rpx;
	height: auto;
	margin: 0rpx auto 30rpx auto;
	padding: 30rpx 0 30rpx 0;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: column;
	flex-direction: column;
	border-radius: 0rpx;
	background: linear-gradient(45deg,rgba(31, 174, 221,0.05),rgba(255,83,77,0.05));
	box-shadow: 0 10rpx 30rpx rgba(0, 0, 0, 0);
	-webkit-align-items: center;
	align-items: center;
	border: 1rpx solid rgba(0, 0, 0, 0.1)
}

.match-detail {
	width: 600rpx;
	height: 100rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: row;
	flex-direction: row;
	-webkit-justify-content: space-between;
	justify-content: space-between
}

.match-detail-no {
	width: 100rpx;
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	font-size: 40rpx;
	font-weight: bolder;
	border-radius: 50%;
	color: rgba(0,52,63,0.5);
	background: rgba(208, 233, 255, 0.6)
}

.match-detail-info {
	width: 480rpx;
	height: 100rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: column;
	flex-direction: column;
	-webkit-align-items: center;
	align-items: center;
	-webkit-justify-content: space-around;
	justify-content: space-around
}

.maatch-detail-name {
	height: auto;
	line-height: 50rpx;
	font-size: 35rpx;
	color: rgba(0,0,0,0.8)
}

.match-set {
	height: 200rpx;
	width: 600rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: column;
	flex-direction: column;
	margin: 20rpx 0 8rpx 0;
	-webkit-justify-content: space-around;
	justify-content: space-around
}

.match-set-teama {
	width: 450rpx;
	height: 80rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: row;
	flex-direction: row;
	-webkit-align-items: center;
	align-items: center;
	background: rgba(37, 198, 252, 0.6);
	border-radius: 0rpx;
}

.teama-name {
	width: 380rpx;
	line-height: 80rpx;
	text-align: center;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.teama-set {
	width: 60rpx;
	height: 60rpx;
	line-height: 60rpx;
	background: rgba(255,83,77,0.8);
	text-align: center;
	border-radius: 50%;
	color: white
}

.match-set-teamb {
	width: 450rpx;
	height: 80rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: row;
	flex-direction: row;
	-webkit-align-items: center;
	align-items: center;
	background: rgba(255,83,77,0.6);
	border-radius: 0rpx;
	margin-left: 150rpx;
}

.teamb-name {
	width: 380rpx;
	line-height: 80rpx;
	text-align: center;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.teamb-set {
	width: 60rpx;
	height: 60rpx;
	line-height: 60rpx;
	background: rgba(37, 198, 252, 0.8);
	text-align: center;
	margin-left: 10rpx;
	border-radius: 50%;
	color: white
}

.match-video {
	height: 200rpx;
	width: 600rpx;
	border-radius: 20rpx;
	background: #fff;
	margin-bottom: 15rpx;
	border: 1rpx solid rgba(0, 0, 0, 0.2)
}

.match-statistic {
	height: 80rpx;
	line-height: 80rpx;
	width: 600rpx;
	text-align: center;
	font-size: 30rpx;
	color: rgba(255, 255, 255, 1);
	background: rgba(241,124,103,0.5);
	border-radius: 0rpx;
}

.match-score {
	height: 60rpx;
	line-height: 60rpx;
	width: 600rpx;
	text-align: left;
	font-size: 30rpx;
	color: rgba(0, 0, 0, 0.7)
}

.match-ref {
	height: 60rpx;
	line-height: 60rpx;
	width: 600rpx;
	text-align: left;
	font-size: 30rpx;
	color: rgba(0, 0, 0, 0.7)
}

.match-location {
	height: 60rpx;
	line-height: 60rpx;
	width: 600rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-direction: row;
	flex-direction: row;
	-webkit-align-items: center;
	align-items: center
}

.match-location-images {
	width: 40rpx;
	height: 40rpx;
}

.match-location-detail {
	height: 60rpx;
	line-height: 60rpx;
	width: 500rpx;
	text-align: left;
	padding: 10rpx;
	font-size: 30rpx;
	color: rgba(0, 0, 0, 0.7)
}